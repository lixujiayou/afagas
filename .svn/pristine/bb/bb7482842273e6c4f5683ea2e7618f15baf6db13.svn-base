/* 登录页面 */

html {
    height: 100%
}
.L-body-bg {
    background: rgb(77, 240, 244);
    background: -moz-linear-gradient(top, rgb(77, 240, 244) 0%, rgb(83, 142, 238) 50%, rgb(124, 87, 228) 100%);
    background: -webkit-linear-gradient(top, rgb(77, 240, 244) 0%, rgb(83, 142, 238) 50%, rgb(124, 87, 228) 100%);
    background: linear-gradient(to bottom, rgb(77, 240, 244) 0%, rgb(83, 142, 238) 50%, rgb(124, 87, 228) 100%);
    filter: progid: DXImageTransform.Microsoft.gradient( startColorstr='#4df0f4', endColorstr='#7c57e4', GradientType=0);
}
.L-body {
    width: 80%;
    margin: 0 auto
}
.logobox {
    padding: 20% 0;
    text-align: center;
}
.logobox img {
    width: 60%;
    max-width: 250px;
}
.L-form-line {
    margin-left: 40px;
    position: relative;
    border-bottom: 1px solid rgba(255, 255, 255, .2);
    margin-bottom: 20px;
}
.L-form-line input[type="text"],
.L-form-line input[type="password"] {
    background: none;
    border: none;
    margin: 0;
    width: 100%;
    height: 48px;
    line-height: 48px;
    color: #fff
}
.L-form-line input[type="text"]::-webkit-input-placeholder {
    /* WebKit browsers */
    color: rgba(255, 255, 255, .5);
}
.L-form-line input[type="text"]:-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: rgba(255, 255, 255, .5);
}
.L-form-line input[type="text"]::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: rgba(255, 255, 255, .5);
}
.L-form-line input[type="text"]:-ms-input-placeholder {
    /* Internet Explorer 10+ */
    color: rgba(255, 255, 255, .5);
}
.L-form-line input[type="password"]::-webkit-input-placeholder {
    /* WebKit browsers */
    color: rgba(255, 255, 255, .5);
}
.L-form-line input[type="password"]:-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: rgba(255, 255, 255, .5);
}
.L-form-line input[type="password"]::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: rgba(255, 255, 255, .5);
}
.L-form-line input[type="password"]:-ms-input-placeholder {
    /* Internet Explorer 10+ */
    color: rgba(255, 255, 255, .5);
}
.getcodebtn {
    width: 100px;
    line-height: 24px;
    position: absolute;
    right: 0;
    top: 0;
    border: none;
    background: rgba(255, 255, 255, .2);
    color: #fff;
}
.uicon {
    display: inline-block;
    width: 30px;
    height: 30px;
    background: url(../image/uicon.png) no-repeat;
}
.ui-1 {
    background-size: 100% auto;
    background-position: 0 0
}
.ui-2 {
    background-size: 100% auto;
    background-position: 0 -30px;
}
.L-form-line .pa {
    left: -36px;
    top: 8px;
}
.L-loginbtn {
    background: #ffffff;
    color: #0284da;
    padding: 15px 12px;
}
.L-checkbox.mui-left input[type=checkbox],
.L-checkbox.mui-left input[type=radio] {
    left: 5px;
}
.L-checkbox.mui-left label {
    padding-right: 15px;
    padding-left: 48px;
    color: #fff
}
.L-checkbox input[type=checkbox]:before,
.L-checkbox input[type=radio]:before {
    color: #fff;
}
.L-checkbox input[type=checkbox]:checked:before,
.L-checkbox input[type=radio]:checked:before {
    color: #fff;
}

/* 首页 */

body {
    background: #e6f0f4;
    font-size: 16px
}
.mui-content {
    background-color: #e6f0f4;
}
.mui-bar-nav {
    top: 0;
    -webkit-box-shadow: none;
    box-shadow: none;
    overflow: hidden;
    border-bottom: 1px solid #dddddd;
    background: #fff;
}
.con {
    background: #fff;
    margin-bottom: 10px;
}
.p-form-line {
    border-bottom: 1px solid #efefef;
    padding: 5px 0;
    position: relative;
}
.p-form-line span {
    display: inline-block;
}
.p-form-line select {
    direction: rtl;
    margin-bottom: 0;
    padding-right: 25px;
}
.p-form-line input {
    padding: 10px 15px;
    margin-bottom: 0;
    border: none;
    text-align: right;
}
.p-form-line .sel-label {
    position: absolute;
    right: 10px;
    top: 15px;
}
.p-form-line .sel-label:after {
    font: normal normal normal 14px/1 FontAwesome;
    content: "\f0d7";
    color: #bfbfbf;
    margin-left: 10px;
}
.btn-primary {
    background: #70a8ff;
    border: 1px solid #70a8ff;
}
.mui-btn-blue.mui-active:enabled,
.mui-btn-blue:enabled:active,
.mui-btn-primary.mui-active:enabled,
.mui-btn-primary:enabled:active,
input[type=submit].mui-active:enabled,
input[type=submit]:enabled:active {
    color: #fff;
    border: 1px solid #54a9f1;
    background-color: #54a9f1;
}
.mui-btn-block {
    padding: 10px 0;
}
.borderbottom {
    box-shadow: 0 2px 2px #d5dbdd
}
.con-head {
    padding: 15px 15px;
    font-size: 16px;
    font-weight: normal;
    border-bottom: 1px solid #e6f0f4;
    margin: 0
}
.adrlist li.mui-table-view-cell:after {
    position: absolute;
    right: 0;
    bottom: 0;
    left: 15px;
    height: 1px;
    border-bottom: dashed 1px #e6f0f4;
    content: '';
    -webkit-transform: scaleY(1);
    transform: scaleY(1);
    background-color: #fff;
}
.adrlist li.mui-table-view-cell {
    color: #808080
}
.mui-badge {
    padding: 5px 10px;
    color: #666666;
    background: #e0e0e0;
}
.adrlist li.mui-table-view-cell .mui-badge {
    right: 0
}
.adrlist:before,
.adrlist:after {
    display: none;
}
.adrlist li.mapicon {
    padding-left: 40px;
    background: url(../image/map.png) 10px 13px no-repeat;
    background-size: 20px auto;
    font-size: 16px;
}
.mui-badge-blue,
.mui-badge-primary {
    color: #fff;
    background-color: #64b8ff;
}
.mui-badge-success {
    color: #fff;
    background-color: #62d83c;
}
.mui-badge-warning {
    color: #fff;
    background-color: #ffc011;
}
.mui-badge-danger {
    color: #fff;
    background-color: #ff7979;
}

/* 审核流程 */

.flowlist {
    margin: 0 !important;
}
.flowlist img {
    vertical-align: middle;
}
.flowlist .mui-table-view-cell {
    padding: 20px 15px 20px 15px;
    margin-left: 40px;
    overflow: visible;
}
.flowlist .mui-table-view-cell img {
    position: absolute;
    left: -25px;
    top: 20px;
}
.flowlist:after {
    display: none;
}
.flowlist .mui-table-view-cell:before {
    content: '';
    display: inline-block;
    position: absolute;
    top: 0;
    bottom: 0;
    width: 1px;
    background: #c8c8c8;
    left: -13px;
}
.flowlist .mui-table-view-cell:last-child:before {
    height: 100%;
}

/* 机房及配套信息 */

.jifinfolist li {
    box-sizing: border-box;
    padding-left: 15px;
    min-height: 48px;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    position: relative;
}
.jifinfolist li .item-media {
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-flex: 0;
    -webkit-flex-shrink: 0;
    -ms-flex: 0 0 auto;
    flex-shrink: 0;
    -webkit-box-lines: single;
    -moz-box-lines: single;
    -webkit-flex-wrap: nowrap;
    -ms-flex-wrap: none;
    -ms-flex-wrap: nowrap;
    flex-wrap: nowrap;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    box-sizing: border-box;
    padding-top: 10px;
    padding-bottom: 10px;
    padding-right: 10px;
}
.jifinfolist li .item-inner {
    padding-right: 16px;
    position: relative;
    width: 100%;
    padding-top: 8px;
    padding-bottom: 8px;
    min-height: 48px;
    box-sizing: border-box;
    -webkit-box-flex: 1;
    -ms-flex: 1;
    min-width: 0;
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    -webkit-justify-content: space-between;
    justify-content: space-between;
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-align-items: center;
    align-items: center;
    -ms-flex-item-align: stretch;
    -webkit-align-self: stretch;
    align-self: stretch;
}
.jifinfolist li select {
    margin-bottom: 0;
    text-align: right;
    width: 100%;
    position: relative;
    z-index: 2;
    background: transparent;
    direction: rtl;
    padding-left: 0;
    padding-right: 10px;
}
.jifinfolist li input {
    margin-bottom: 0;
    width: 100%;
    position: relative;
    z-index: 2;
    background: transparent;
    padding-left: 0;
    padding-right: 10px;
}
.jifinfolist li textarea {
    margin-bottom: 0;
    width: 100%;
    position: relative;
    z-index: 2;
    background: transparent;
    padding-left: 0;
    padding-right: 10px;
}

.jifinfolist .item-inner .sel-label {
    position: absolute;
    right: 10px;
    top: 18px;
    color: #808080;
    z-index: 1
}
.jifinfolist .item-inner .sel-label:after {
    font: normal normal normal 14px/1 FontAwesome;
    content: "\f0d7";
    color: #bfbfbf;
    margin-left: 10px;
}
.btn-around {
    display: inline-block;
    width: 26px;
    height: 26px;
    border-radius: 100%;
    background: #64b8ff;
    text-align: center;
    line-height: 26px;
    color: #fff;
    font-size: 14px;
}
.btn-around.active {
    background: #ff5555;
}
.item-title {
    font-size: 14px;
    line-height: 20px;
    position: relative;
    overflow: hidden;
    max-height: 40px;
    text-overflow: ellipsis;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    display: -webkit-box;
}
.jifinfolist li:after {
    content: '';
    position: absolute;
    left: 15px;
    bottom: 0;
    right: 15px;
    top: auto;
    height: 1px;
    background-color: #efefef;
    display: block;
    z-index: 15;
    -webkit-transform-origin: 50% 100%;
    transform-origin: 50% 100%;
}
.mui-table-view-cell>.badge{
    position: absolute;
    top: 50%;
    right: 15px;
    color: #808080;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}
